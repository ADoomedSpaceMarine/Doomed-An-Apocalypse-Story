Actor DebrisStuffs
{
+FIXMAPTHINGPOS
+LOOKALLAROUND
+NOTAUTOAIMED
+MISSILE
+NOBLOCKMAP +MOVEWITHSECTOR
+NOGRAVITY
+DROPOFF
+NOTELEPORT
+FORCEXYBILLBOARD
//+NOTDMATCH
+GHOST
+THRUACTORS
+FLOORCLIP
renderstyle Translucent
alpha 1.0
radius 1
height 1
mass 1
damage 0
}

Actor GenericCasing : DebrisStuffs
{
bouncetype "doom"
seesound ""
-NOGRAVITY
+MOVEWITHSECTOR
Radius 1
Height 1
bouncefactor 0.5
Scale .15
}


ACTOR PistolCasing : GenericCasing
{
Scale .15
bouncesound "weapons/casing"
States
	{
	Spawn:
		TNT1 A 0
		TNT1 A 0 A_QueueCorpse
		TNT1 A 0 A_Jump(256,1,2,3,4,5,6,7,8)
		CAS1 ABCDEFGH 1 A_JumpIf(waterlevel > 0, "Death")
		TNT1 A 0
		TNT1 A 0 A_JumpIf(velz == 0,"Death")
		goto spawn+3
	Death:
		TNT1 A 0
		TNT1 A 0 A_Stop
		TNT1 A 0 A_Jump(128,2)
		TNT1 A 0 A_SetScale(scalex*(-1),scaley)
		TNT1 A 0 A_jump(256,1,2,3)
		CAS1 I 30
		wait
		CAS1 J 30
		wait
		CAS1 K 30
		wait
	}
}

Actor PistolCasingSpawner
{
projectile
-ACTIVATEIMPACT
-ACTIVATEPCROSS
speed 10
states
	{
	Spawn:
		TNT1 A 3 NoDelay
		TNT1 A 0 A_SPawnItemEx("PistolCasing",0,0,0,random(3,5),0,random(4,6),	random(-80,-90))
		stop
	Death:
		stop
	}
}

