//Menu Items
menu/activate		MSELECT	// Activate a new menu
menu/backup			MBACK	// Backup to previous menu
menu/prompt			MSELECT	// Activate a prompt "menu"
$random menu/cursor			{menu/MenuCurs1 menu/MenuCurs2 menu/MenuCurs3 menu/MenuCurs4 menu/MenuCurs5 menu/MenuCurs6}// Move cursor up/down
menu/change			UPDOWN4	// Select new value for option
menu/invalid		MUNAVA		// Menu not available
menu/dismiss		MUNAVA	// Dismiss a prompt message
menu/choose			MSELECT	// Choose a menu item
menu/clear			MUNAVA	// Close top menu
menu/quit1 			GMQUIT
menu/quit2 			GMQUIT

